generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Timeline {
  id        String   @id @default(cuid())
  date      DateTime
  actual    String
  forecast  String
  value     Float    @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}